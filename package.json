{
  "name": "testx-postgres-keywords",
  "version": "2.0.0",
  "description": "Keywords to test postgres db with testx.io",
  "main": "index.coffee",
  "dependencies": {
    "pg": "^6.0.3",
    "pg-connection-string": "^0.1.3",
    "q": "^1.4.1"
  },
  "peerDependencies": {
    "testx": "^2.3.1"
  },
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "testx": "^2.3.1"
  },
  "scripts": {
    "test-local": "protractor test/conf.coffee",
    "pretest": "docker run -d --name postgres-testx postgres:9-alpine; sleep 5",
    "test": " docker run --rm -it -v `pwd`:/work --link postgres-testx testx/protractor test/conf.coffee",
    "posttest": "docker kill postgres-testx && docker rm -v postgres-testx"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/testxio/testx-postgres-keywords.git"
  },
  "keywords": [
    "testx",
    "keywords",
    "postgres",
    "sql",
    "database",
    "db"
  ],
  "author": "Giovanni Renfurm, Gantcho Kojuharov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/testxio/testx-postgres-keywords/issues"
  },
  "homepage": "https://github.com/testxio/testx-postgres-keywords#readme"
}
